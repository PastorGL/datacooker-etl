<a id="index.html"></a>
<h1>${distro} Index</h1>

<h2>Packages</h2>
<table>
    <thead>
    <tr>
        <th>Package</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
        #foreach( $pkg in $pkgs.entrySet() )
        <tr>
            <td><a href="package/${pkg.key}.html">${pkg.key}</a></td>
            <td>${pkg.value}</td>
        </tr>
        #end
    </tbody>
</table>
