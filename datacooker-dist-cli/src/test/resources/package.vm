<a id="package/${name}.html"></a>
<h1>Package <code>${name}</code></h1>
<p>${descr}</p>

#if( $pluggables.size() > 0 )
<h2>${pluggables.size()} Pluggables</h2>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
        #foreach( $plg in $pluggables.entrySet() )
        <tr>
            <td><a href="pluggable/${plg.key}.html">${plg.key}</a></td>
            <td>#foreach( $k in $plg.value.meta.kind() )$k#if( $foreach.hasNext ), #end#end</td>
            <td>${plg.value.meta.descr}</td>
        </tr>
        #end
    </tbody>
</table>
#end

<p>Back to <a href="index.html">index</a></p>
